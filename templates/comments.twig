{% extends "layout.twig" %}
{% block content %}

<section id="prosjekter" class="prosjekter">
    <div class="container container_small">
    </div>
</section>

<section>
    <div class="openContainerNoBorder text" id="commentsTable">
        <h3>Kommentarer </h3>
        <table class="table" id="table">
            {% if hours %}
                <tr>
                    <th>Prosjektnavn</th>
                    <th>Navn</th>
                    <th>Kommentar</th>

                </tr>
                {% for Hour in hours %}
                    <tr>
                        {#PROSJEKTNAVN#}
                        <td>{{ Hour.getHourID() }}</td>

                        {#Navn#}
                        <td>{{ Hour.getWhoworked }}</td>
                        <td>{{ Hour.getComment() }}</td>

                    </tr>
                {% endfor %}
            {% endif %}
        </table>
    </div>
</section>
{% endblock %}